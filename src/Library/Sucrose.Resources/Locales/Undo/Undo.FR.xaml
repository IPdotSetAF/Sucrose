<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Undo.QuestionTitle">Sucrose Undo</system:String>

    <system:String x:Key="Undo.QuestionMessage">Voulez-vous vraiment désinstaller Sucrose Wallpaper Engine?</system:String>
    <system:String x:Key="Undo.QuestionDescription">Pour supprimer avec tous les paramètres, sélectionnez Oui; pour conserver les paramètres et supprimer seulement, sélectionnez Non; et pour annuler le processus de suppression, sélectionnez Annuler.</system:String>
</ResourceDictionary>