<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Undo.QuestionTitle">Sucrose Undo</system:String>

    <system:String x:Key="Undo.QuestionMessage">Tem certeza de que deseja desinstalar o Sucrose Wallpaper Engine?</system:String>
    <system:String x:Key="Undo.QuestionDescription">Para remover com todas as configurações, selecione Sim; para manter as configurações e remover apenas, selecione Não; e para cancelar o processo de remoção, selecione Cancelar.</system:String>
</ResourceDictionary>