<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Wizard.WindowTitle">Sucrose Wizard</system:String>

    <system:String x:Key="Wizard.ReloadText">重试</system:String>

    <system:String x:Key="Wizard.MessageText.Preparing">准备必要的准备工作</system:String>

    <system:String x:Key="Wizard.MessageText.Temporary">检查临时位置</system:String>
    <system:String x:Key="Wizard.MessageText.Temporary.Error">无法检查临时位置</system:String>

    <system:String x:Key="Wizard.MessageText.Connection">检查网络连接</system:String>
    <system:String x:Key="Wizard.MessageText.Connection.None">没有网络连接</system:String>
    <system:String x:Key="Wizard.MessageText.Connection.Error">无法检查网络连接</system:String>

    <system:String x:Key="Wizard.MessageText.Searching">搜索所需文件</system:String>
    <system:String x:Key="Wizard.MessageText.Searching.Not">未找到所需文件</system:String>
    <system:String x:Key="Wizard.MessageText.Searching.Error">无法搜索所需文件</system:String>

    <system:String x:Key="Wizard.MessageText.Listing">获取所需列表</system:String>
    <system:String x:Key="Wizard.MessageText.Listing.Empty">所需列表为空</system:String>
    <system:String x:Key="Wizard.MessageText.Listing.Error">无法获取所需列表</system:String>

    <system:String x:Key="Wizard.MessageText.Filtering">列表正在过滤</system:String>
    <system:String x:Key="Wizard.MessageText.Filtering.Not">无法过滤列表</system:String>
    <system:String x:Key="Wizard.MessageText.Filtering.Error">无法过滤列表</system:String>

    <system:String x:Key="Wizard.MessageText.Downloading">下载所需文件</system:String>
    <system:String x:Key="Wizard.MessageText.Downloading.Error">无法下载所需文件</system:String>
    <system:String x:Key="Wizard.MessageText.Downloading.Complete.Cancel">所需文件的下载已取消</system:String>
    <system:String x:Key="Wizard.MessageText.Downloading.Complete.Error">下载所需文件时出现错误</system:String>

    <system:String x:Key="Wizard.MessageText.Extracting">提取已下载的文件</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Executing">正在执行已提取的文件</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Executed">已执行提取的文件</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Failed">已下载的文件失败</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Corrupt">已下载的文件损坏</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Not">已下载的文件无法使用</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Error">无法提取已下载的文件</system:String>
</ResourceDictionary>