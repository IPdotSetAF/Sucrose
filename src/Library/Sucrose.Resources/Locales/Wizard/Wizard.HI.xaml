    <system:String x:Key="Wizard.MessageText.Extracting.Corrupt">डाउनलोड की गई फ़ाइल कोरप्ट है</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Not">डाउनलोड की गई फ़ाइल उपयोग योग्य नहीं है</system:String>
