<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Wizard.WindowTitle">Sucrose Wizard</system:String>

    <system:String x:Key="Wizard.ReloadText">再試行</system:String>

    <system:String x:Key="Wizard.MessageText.Preparing">必要な準備をしています</system:String>

    <system:String x:Key="Wizard.MessageText.Temporary">一時的な場所をチェックしています</system:String>
    <system:String x:Key="Wizard.MessageText.Temporary.Error">一時的な場所をチェックできませんでした</system:String>

    <system:String x:Key="Wizard.MessageText.Connection">インターネット接続をチェックしています</system:String>
    <system:String x:Key="Wizard.MessageText.Connection.None">インターネットに接続されていません</system:String>
    <system:String x:Key="Wizard.MessageText.Connection.Error">インターネット接続をチェックできませんでした</system:String>

    <system:String x:Key="Wizard.MessageText.Searching">必要なファイルを検索しています</system:String>
    <system:String x:Key="Wizard.MessageText.Searching.Not">必要なファイルが見つかりません</system:String>
    <system:String x:Key="Wizard.MessageText.Searching.Error">必要なファイルを検索できませんでした</system:String>

    <system:String x:Key="Wizard.MessageText.Listing">必要なリストを取得しています</system:String>
    <system:String x:Key="Wizard.MessageText.Listing.Empty">必要なリストは空です</system:String>
    <system:String x:Key="Wizard.MessageText.Listing.Error">必要なリストを受信できませんでした</system:String>

    <system:String x:Key="Wizard.MessageText.Filtering">リストをフィルタリングしています</system:String>
    <system:String x:Key="Wizard.MessageText.Filtering.Not">リストをフィルタリングできません</system:String>
    <system:String x:Key="Wizard.MessageText.Filtering.Error">リストをフィルタリングできませんでした</system:String>

    <system:String x:Key="Wizard.MessageText.Downloading">必要なファイルをダウンロードしています</system:String>
    <system:String x:Key="Wizard.MessageText.Downloading.Error">必要なファイルをダウンロードできませんでした</system:String>
    <system:String x:Key="Wizard.MessageText.Downloading.Complete.Cancel">必要なファイルのダウンロードがキャンセルされました</system:String>
    <system:String x:Key="Wizard.MessageText.Downloading.Complete.Error">必要なファイルのダウンロード中にエラーが発生しました</system:String>

    <system:String x:Key="Wizard.MessageText.Extracting">ダウンロードしたファイルを展開しています</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Executing">展開されたファイルが実行されています</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Executed">展開されたファイルが実行されました</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Failed">ダウンロードしたファイルが失敗しました</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Corrupt">ダウンロードしたファイルが壊れています</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Not">ダウンロードしたファイルは使用できません</system:String>
    <system:String x:Key="Wizard.MessageText.Extracting.Error">ダウンロードしたファイルを展開できませんでした</system:String>
</ResourceDictionary>