    <system:String x:Key="Property.ColorPicker.Popup.Title">Sucrose Property Color Picker</system:String>

    <system:String x:Key="Property.FileDropDown.Tip">Pilih sebuah file</system:String>
    <system:String x:Key="Property.FileDropDown.Filter">Semua File (*.*)|*.*</system:String>

    <system:String x:Key="Property.FileDropDown.Access.Close">Mengerti</system:String>
