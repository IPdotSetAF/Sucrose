<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Property.WindowTitle">Sucrose Property</system:String>

    <system:String x:Key="Property.DeleteTip">Hapus Salinan</system:String>
    <system:String x:Key="Property.RefreshTip">Segarkan Antarmuka</system:String>
    <system:String x:Key="Property.RestoreTip">Pulihkan Default</system:String>

    <system:String x:Key="Property.Type.NotSupport">Tipe kontrol '{0}' tidak didukung.</system:String>

    <system:String x:Key="Property.ColorPicker.Color">Warna</system:String>
    <system:String x:Key="Property.ColorPicker.Cancel">Batal</system:String>
    <system:String x:Key="Property.ColorPicker.Confirm">Konfirmasi</system:String>
