<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Property.WindowTitle">Sucrose Property</system:String>

    <system:String x:Key="Property.DeleteTip">Slet kopi</system:String>
    <system:String x:Key="Property.RefreshTip">Opdater grænseflade</system:String>
    <system:String x:Key="Property.RestoreTip">Gendan standard</system:String>

    <system:String x:Key="Property.Type.NotSupport">Kontroltype '{0}' understøttes ikke.</system:String>

    <system:String x:Key="Property.ColorPicker.Color">Farve</system:String>
    <system:String x:Key="Property.ColorPicker.Cancel">Annuller</system:String>
    <system:String x:Key="Property.ColorPicker.Confirm">Bekræft</system:String>
    <system:String x:Key="Property.ColorPicker.EyeDropper">Pipette</system:String>
    <system:String x:Key="Property.ColorPicker.Popup.Title">Sucrose Property Color Picker</system:String>

    <system:String x:Key="Property.FileDropDown.Tip">Vælg en fil</system:String>
    <system:String x:Key="Property.FileDropDown.Filter">Alle filer (*.*)|*.*</system:String>

    <system:String x:Key="Property.FileDropDown.Access.Close">Forstået</system:String>
    <system:String x:Key="Property.FileDropDown.Access.Title">Adgangsvarsel</system:String>
    <system:String x:Key="Property.FileDropDown.Access.Message">Du har ikke tilladelse til at få adgang til den valgte fil.</system:String>
