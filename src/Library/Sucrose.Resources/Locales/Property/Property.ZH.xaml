<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Property.WindowTitle">Sucrose Property</system:String>

    <system:String x:Key="Property.DeleteTip">删除副本</system:String>
    <system:String x:Key="Property.RefreshTip">刷新界面</system:String>
    <system:String x:Key="Property.RestoreTip">恢复默认</system:String>

    <system:String x:Key="Property.Type.NotSupport">不支持控件类型 '{0}'。</system:String>

    <system:String x:Key="Property.ColorPicker.Color">颜色</system:String>
    <system:String x:Key="Property.ColorPicker.Cancel">取消</system:String>
    <system:String x:Key="Property.ColorPicker.Confirm">确认</system:String>
    <system:String x:Key="Property.ColorPicker.EyeDropper">取色器</system:String>
    <system:String x:Key="Property.ColorPicker.Popup.Title">Sucrose Property Color Picker</system:String>

    <system:String x:Key="Property.FileDropDown.Tip">选择文件</system:String>
    <system:String x:Key="Property.FileDropDown.Filter">所有文件 (*.*)|*.*</system:String>

    <system:String x:Key="Property.FileDropDown.Access.Close">明白</system:String>
    <system:String x:Key="Property.FileDropDown.Access.Title">访问警告</system:String>
    <system:String x:Key="Property.FileDropDown.Access.Message">您没有权限访问所选文件。</system:String>
