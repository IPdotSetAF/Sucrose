<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Update.WindowTitle">Sucrose Update</system:String>

    <system:String x:Key="Update.HelpText">解决问题</system:String>

    <system:String x:Key="Update.ReloadText">重试</system:String>

    <system:String x:Key="Update.ReloadText.Browser.Opened">在浏览器中打开</system:String>
    <system:String x:Key="Update.ReloadText.Browser.Opening">正在浏览器中打开</system:String>

    <system:String x:Key="Update.MessageText.Preparing">准备必要的准备工作</system:String>

    <system:String x:Key="Update.MessageText.Temporary">检查临时位置</system:String>
    <system:String x:Key="Update.MessageText.Temporary.Error">无法检查临时位置</system:String>

    <system:String x:Key="Update.MessageText.Connection">检查网络连接</system:String>
    <system:String x:Key="Update.MessageText.Connection.None">没有网络连接</system:String>
    <system:String x:Key="Update.MessageText.Connection.Error">无法检查网络连接</system:String>

    <system:String x:Key="Update.MessageText.Listing">获取所需列表</system:String>
    <system:String x:Key="Update.MessageText.Listing.Empty">所需列表为空</system:String>
    <system:String x:Key="Update.MessageText.Listing.Error">无法获取所需列表</system:String>

    <system:String x:Key="Update.MessageText.Filtering">列表正在过滤</system:String>
    <system:String x:Key="Update.MessageText.Filtering.Not">无法过滤列表</system:String>
    <system:String x:Key="Update.MessageText.Filtering.Error">无法过滤列表</system:String>

    <system:String x:Key="Update.MessageText.Comparing">正在比较列表</system:String>
    <system:String x:Key="Update.MessageText.Comparing.Update">应用程序已是最新</system:String>
    <system:String x:Key="Update.MessageText.Comparing.Error">无法比较列表</system:String>

    <system:String x:Key="Update.MessageText.Searching">搜索所需文件</system:String>
    <system:String x:Key="Update.MessageText.Searching.Not">未找到所需文件</system:String>
    <system:String x:Key="Update.MessageText.Searching.Error">无法搜索所需文件</system:String>
    <system:String x:Key="Update.MessageText.Searching.Condition">未找到所需文件</system:String>

    <system:String x:Key="Update.MessageText.Downloading">下载所需文件</system:String>
    <system:String x:Key="Update.MessageText.Downloading.Error">无法下载所需文件</system:String>
    <system:String x:Key="Update.MessageText.Downloading.Complete.Cancel">所需文件的下载已取消</system:String>
    <system:String x:Key="Update.MessageText.Downloading.Complete.Error">下载所需文件时出现错误</system:String>

    <system:String x:Key="Update.MessageText.Running">正在准备下载的文件</system:String>
    <system:String x:Key="Update.MessageText.Running.Executing">正在执行下载的文件</system:String>
    <system:String x:Key="Update.MessageText.Running.Executed">已执行下载的文件</system:String>
    <system:String x:Key="Update.MessageText.Running.Not">下载的文件不可用</system:String>
    <system:String x:Key="Update.MessageText.Running.Error">无法准备下载的文件</system:String>

    <system:String x:Key="Update.MessageText.Extracting">提取已下载的文件</system:String>
    <system:String x:Key="Update.MessageText.Extracting.Executing">正在执行已提取的文件</system:String>
    <system:String x:Key="Update.MessageText.Extracting.Executed">已执行提取的文件</system:String>
    <system:String x:Key="Update.MessageText.Extracting.Failed">已下载的文件失败</system:String>
    <system:String x:Key="Update.MessageText.Extracting.Corrupt">已下载的文件损坏</system:String>
    <system:String x:Key="Update.MessageText.Extracting.Not">已下载的文件无法使用</system:String>
    <system:String x:Key="Update.MessageText.Extracting.Error">无法提取已下载的文件</system:String>
</ResourceDictionary>