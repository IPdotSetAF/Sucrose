    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="FullLibraryPage/Portal.FullLibraryPage.NL.xaml" />
        <ResourceDictionary Source="EmptyLibraryPage/Portal.EmptyLibraryPage.NL.xaml" />
        <ResourceDictionary Source="SearchLibraryPage/Portal.SearchLibraryPage.NL.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <system:String x:Key="Portal.LibraryPage.Title">LibraryPage</system:String>

    <system:String x:Key="Portal.LibraryPage.Import.Successful">Het thema genaamd '{0}' is succesvol toegevoegd aan de bibliotheek.</system:String>
    <system:String x:Key="Portal.LibraryPage.Import.Unsuccessful">Het thema genaamd '{0}' kon niet aan de bibliotheek worden toegevoegd. Reden: '{1}'.</system:String>
</ResourceDictionary>