<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="FullLibraryPage/Portal.FullLibraryPage.FR.xaml" />
        <ResourceDictionary Source="EmptyLibraryPage/Portal.EmptyLibraryPage.FR.xaml" />
        <ResourceDictionary Source="SearchLibraryPage/Portal.SearchLibraryPage.FR.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <system:String x:Key="Portal.LibraryPage.Title">LibraryPage</system:String>

    <system:String x:Key="Portal.LibraryPage.Import.Successful">Le thème '{0}' a été ajouté avec succès à la bibliothèque.</system:String>
    <system:String x:Key="Portal.LibraryPage.Import.Unsuccessful">Le thème '{0}' n'a pas pu être ajouté à la bibliothèque. Raison: '{1}'.</system:String>
</ResourceDictionary>