<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="FullLibraryPage/Portal.FullLibraryPage.EN.xaml" />
        <ResourceDictionary Source="EmptyLibraryPage/Portal.EmptyLibraryPage.EN.xaml" />
        <ResourceDictionary Source="SearchLibraryPage/Portal.SearchLibraryPage.EN.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <system:String x:Key="Portal.LibraryPage.Title">LibraryPage</system:String>

    <system:String x:Key="Portal.LibraryPage.Import.Successful">The theme '{0}' has been successfully added to the library.</system:String>
    <system:String x:Key="Portal.LibraryPage.Import.Unsuccessful">The theme '{0}' couldn't be added to the library. Reason: '{1}'.</system:String>
</ResourceDictionary>