<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="FullLibraryPage/Portal.FullLibraryPage.KO.xaml" />
        <ResourceDictionary Source="EmptyLibraryPage/Portal.EmptyLibraryPage.KO.xaml" />
        <ResourceDictionary Source="SearchLibraryPage/Portal.SearchLibraryPage.KO.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <system:String x:Key="Portal.LibraryPage.Title">LibraryPage</system:String>

    <system:String x:Key="Portal.LibraryPage.Import.Successful">'{0}' 테마가 성공적으로 라이브러리에 추가되었습니다.</system:String>
    <system:String x:Key="Portal.LibraryPage.Import.Unsuccessful">'{0}' 테마를 라이브러리에 추가할 수 없었습니다. 이유: '{1}'.</system:String>
</ResourceDictionary>