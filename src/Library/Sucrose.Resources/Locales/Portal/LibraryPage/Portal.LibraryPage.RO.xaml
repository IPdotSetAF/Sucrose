    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="FullLibraryPage/Portal.FullLibraryPage.RO.xaml" />
        <ResourceDictionary Source="EmptyLibraryPage/Portal.EmptyLibraryPage.RO.xaml" />
        <ResourceDictionary Source="SearchLibraryPage/Portal.SearchLibraryPage.RO.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <system:String x:Key="Portal.LibraryPage.Title">LibraryPage</system:String>

    <system:String x:Key="Portal.LibraryPage.Import.Successful">Tema '{0}' a fost adăugată cu succes în bibliotecă.</system:String>
    <system:String x:Key="Portal.LibraryPage.Import.Unsuccessful">Tema '{0}' nu a putut fi adăugată în bibliotecă. Motiv: '{1}'.</system:String>
</ResourceDictionary>