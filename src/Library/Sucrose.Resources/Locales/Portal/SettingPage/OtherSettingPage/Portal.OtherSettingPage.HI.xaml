<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.OtherSettingPage.Title">OtherSettingPage</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook">Discord</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.Description">Sucrose, Discord के साथ संचालन संवाद सुनिश्चित करता है।</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordRefresh">कनेक्शन रिफ्रेश:</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordDelay">रिफ्रेश अवधि (सेकंड):</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Agent">इंटरनेट प्रतिष्ठा</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Agent.Description">इंटरनेट कनेक्शन की आवश्यकता होने पर आपकी प्रतिष्ठा.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key">व्यक्तिगत पहुंच टोकन</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.Description">स्टोर और ऐप्डेट को सही तरीके से उपयोग करने के लिए आवश्यक।</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.HintKey">अगर आपको व्यक्तिगत पहुँच कुंजी प्राप्त करने का तरीका नहीं पता है तो यहाँ क्लिक करें।</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey">कृपया एक व्यक्तिगत पहुँच कुंजी दर्ज करें</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey.Valid">कृपया एक मान्य व्यक्तिगत पहुँच कुंजी दर्ज करें</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Channel">अपडेट चैनल</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Channel.Description">अपडेट प्रक्रिया किस चैनल के माध्यम से की जाएगी या नहीं।</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update">अपडेट विधि</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Update.Description">अपडेट प्रक्रिया किस विधि से की जाएगी या नहीं।</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update.Limit">डाउनलोड बैंडविड्थ सीमा:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer">डेवलपर टूल</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Developer.Description">WebView और CefSharp इंजन के लिए डेवलपर टूल्स का प्रबंधन करें।</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperMode">डिफ़ॉल्ट डेवलपर टूल्स:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperPort">डीबगिंग पोर्ट:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperHint">संकेत: समर्थित थीमों में डिफ़ॉल्ट डेवलपर टूल्स सही तरह से काम नहीं कर सकते हैं। डीबगिंग पोर्ट काम करने के लिए अधिक प्रवृत्त होता है। जब आप तैयार हों, तो आप 'http://localhost:{0}' पता या Chrome ब्राउज़र के लिए 'chrome://inspect', Opera ब्राउज़र के लिए 'opera://inspect', Edge ब्राउज़र के लिए 'edge://inspect' का उपयोग कर सकते हैं।</system:String>
</ResourceDictionary>