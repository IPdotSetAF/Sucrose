<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.SystemSettingPage.Title">SystemSettingPage</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LogTemporary">Log Files</system:String>
    <system:String x:Key="Portal.SystemSettingPage.LogTemporary.Description">Manage all log files generated by the application.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LogTemporary.Create">Create</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LogTemporary.Delete">Clear</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LogTemporary.Hint">Note: Total space occupied by log files is {0}. All applications will be closed only when cleaning starts.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.CacheTemporary">Clear Cache</system:String>
    <system:String x:Key="Portal.SystemSettingPage.CacheTemporary.Description">Clear all cache files created by the application.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.CacheTemporary.Start">Start Cleaning</system:String>

    <system:String x:Key="Portal.SystemSettingPage.CacheTemporary.Hint">Note: Total space occupied by cache files is {0}. All applications will be closed only when cleaning starts.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.StoreTemporary">Clear Store</system:String>
    <system:String x:Key="Portal.SystemSettingPage.StoreTemporary.Description">Clear all store files created by the application.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.StoreTemporary.Start">Start Cleaning</system:String>

    <system:String x:Key="Portal.SystemSettingPage.StoreTemporary.Hint">Note: Total space occupied by store files is {0}. All applications will be closed only when cleaning starts.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LibraryTemporary">Clear Library</system:String>
    <system:String x:Key="Portal.SystemSettingPage.LibraryTemporary.Description">Clear all theme files in your library, whether in your library or created by you.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LibraryTemporary.Start">Start Cleaning</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LibraryTemporary.Hint">Note: Total space occupied by themes in your library is {0}. All applications will be closed only when cleaning starts.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingBackup">Backup Settings</system:String>
    <system:String x:Key="Portal.SystemSettingPage.SettingBackup.Description">Manage all setting files generated by the application.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingBackup.Export">Backup</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingBackup.Import">Restore</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingBackup.Hint">Note: All applications will be closed when the selected operation starts.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingReset">Reset Settings</system:String>
    <system:String x:Key="Portal.SystemSettingPage.SettingReset.Description">Reset all setting files generated by the application.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingReset.Start">Start Reset</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingReset.Hint">Note: All applications will be closed when the reset process starts.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.Log.SaveDialogTitle">Save Log File</system:String>
    <system:String x:Key="Portal.SystemSettingPage.Log.SaveDialogFilter">ZIP Files (*.zip)|*.zip|All Files (*.*)|*.*</system:String>
</ResourceDictionary>