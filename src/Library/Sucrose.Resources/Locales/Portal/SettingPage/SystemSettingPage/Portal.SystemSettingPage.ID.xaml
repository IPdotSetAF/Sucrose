<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.SystemSettingPage.Title">SystemSettingPage</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LogTemporary">File Harian</system:String>
    <system:String x:Key="Portal.SystemSettingPage.LogTemporary.Description">Mengelola semua file harian yang dibuat oleh aplikasi.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LogTemporary.Create">Buat</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LogTemporary.Delete">Hapus</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LogTemporary.Hint">Catatan: Total ruang yang digunakan oleh file harian adalah {0}. Semua aplikasi akan ditutup saat pembersihan dimulai.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.CacheTemporary">Pembersihan Cache</system:String>
    <system:String x:Key="Portal.SystemSettingPage.CacheTemporary.Description">Menghapus semua file cache yang dibuat oleh aplikasi.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.CacheTemporary.Start">Mulai Pembersihan</system:String>

    <system:String x:Key="Portal.SystemSettingPage.CacheTemporary.Hint">Catatan: Total ruang yang digunakan oleh file cache adalah {0}. Semua aplikasi akan ditutup saat pembersihan dimulai.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.StoreTemporary">Pembersihan Toko</system:String>
    <system:String x:Key="Portal.SystemSettingPage.StoreTemporary.Description">Menghapus semua file toko yang dibuat oleh aplikasi.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.StoreTemporary.Start">Mulai Pembersihan</system:String>

    <system:String x:Key="Portal.SystemSettingPage.StoreTemporary.Hint">Catatan: Total ruang yang digunakan oleh file toko adalah {0}. Semua aplikasi akan ditutup saat pembersihan dimulai.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LibraryTemporary">Pembersihan Perpustakaan</system:String>
    <system:String x:Key="Portal.SystemSettingPage.LibraryTemporary.Description">Menghapus semua file tema yang ada atau dibuat di perpustakaan Anda.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LibraryTemporary.Start">Mulai Pembersihan</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LibraryTemporary.Hint">Catatan: Total ruang yang digunakan oleh file tema di perpustakaan Anda adalah {0}. Semua aplikasi akan ditutup saat pembersihan dimulai.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingBackup">File Pengaturan</system:String>
    <system:String x:Key="Portal.SystemSettingPage.SettingBackup.Description">Mengelola semua file pengaturan yang dibuat oleh aplikasi.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingBackup.Export">Cadangkan</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingBackup.Import">Impor Kembali</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingBackup.Hint">Catatan: Semua aplikasi akan ditutup saat tindakan yang Anda pilih dimulai.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingReset">Reset Pengaturan</system:String>
    <system:String x:Key="Portal.SystemSettingPage.SettingReset.Description">Menyetel ulang semua file pengaturan yang dibuat oleh aplikasi.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingReset.Start">Mulai Reset</system:String>

    <system:String x:Key="Portal.SystemSettingPage.SettingReset.Hint">Catatan: Semua aplikasi akan ditutup saat proses reset dimulai.</system:String>

    <system:String x:Key="Portal.SystemSettingPage.Log.SaveDialogTitle">Simpan File Harian</system:String>
    <system:String x:Key="Portal.SystemSettingPage.Log.SaveDialogFilter">File ZIP (*.zip)|*.zip|Semua File (*.*)|*.*</system:String>
</ResourceDictionary>