    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.SystemSettingPage.Title">SystemSettingPage</system:String>

    <system:String x:Key="Portal.SystemSettingPage.LogTemporary">Fișierele Jurnal</system:String>
    <system:String x:Key="Portal.SystemSettingPage.LogTemporary.Description">Gestionarea tuturor fișierelor jurnal create de aplicație.</system:String>

