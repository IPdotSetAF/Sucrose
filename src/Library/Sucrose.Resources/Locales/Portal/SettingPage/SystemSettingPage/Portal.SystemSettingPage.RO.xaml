    <system:String x:Key="Portal.SystemSettingPage.Log.SaveDialogFilter">Fișiere ZIP (*.zip)|*.zip|Toate Fișierele (*.*)|*.*</system:String>

    <system:String x:Key="Portal.SystemSettingPage.Access.Close">Înțeles</system:String>
    <system:String x:Key="Portal.SystemSettingPage.Access.Title">Avertisment de Acces</system:String>
