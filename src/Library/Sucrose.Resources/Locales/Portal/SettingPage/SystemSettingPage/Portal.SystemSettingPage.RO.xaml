    <system:String x:Key="Portal.SystemSettingPage.CacheTemporary">Ștergere Cache</system:String>
    <system:String x:Key="Portal.SystemSettingPage.CacheTemporary.Description">Ștergerea tuturor fișierelor cache create de aplicație.</system:String>

