    <system:String x:Key="Portal.SystemSettingPage.SettingBackup">Fișiere Setări</system:String>
    <system:String x:Key="Portal.SystemSettingPage.SettingBackup.Description">Gestionarea tuturor fișierelor de setări create de aplicație.</system:String>

