<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.GeneralSettingPage.Title">GeneralSettingPage</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.ApplicationLanguage">Bahasa Aplikasi</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationLanguage.Description">Pilih bahasa yang ingin Anda lihat aplikasinya.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup">Mulai Aplikasi</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Description">Untuk memainkan wallpaper, Sucrose perlu berjalan di latar belakang pada permulaan.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.None">Tidak Ada</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.Normal">Normal</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.Priority">Prioritas</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.Scheduler">Penjadwal</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Warning.Close">Faham</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Warning.Title">Amaran Hak Pentadbir</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Warning.Message">Anda memerlukan hak pentadbir untuk menukar status permulaan.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon">Ikon Notifikasi</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.Description">Tray sistem yang terlihat. Sucrose akan terus berjalan secara tersembunyi.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.Notify.Show">Tampilkan</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.Notify.Hide">Sembunyikan</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.NotifyExit">Tutup tray sistem, tutup semua aplikasi Sucrose</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop">Latar Belakang Jendela</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.Description">Menggunakan untuk menyesuaikan latar belakang jendela aplikasi.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackgroundImage.Title">Pilih Gambar Latar Belakang</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackgroundImage.Select">Pilih gambar latar belakang</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackgroundImage.Filter">File Gambar (*.png;*.jpg;*.jpeg;*.tiff;*.webp;*.gif)|*.png;*.jpg;*.jpeg;*.tiff;*.webp;*.gif</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackgroundImage.Access">Maaf, anda tidak mempunyai kebenaran untuk mengakses fail imej yang dipilih</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackdropStretch">Peregangan Latar Belakang:</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackdropOpacity">Keburaman Latar Belakang (%):</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.EngineVolume">Tingkat Volume</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.EngineVolume.Description">Tingkat volume yang berlaku untuk semua wallpaper.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.EngineVolume.VolumeDesktop">Mainkan suara hanya ketika fokus di desktop</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.EngineVolume.VolumeActive">Mainkan bunyi hanya apabila semua aplikasi dibisukan</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.EngineVolume.VolumeSensitivity">Kependaman (Saat):</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.PrivateLibrary">Lokasi Perpustakaan</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.PrivateLibrary.Description">Lokasi penyimpanan tema yang ditambahkan ke perpustakaan.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.PrivateLibrary.LibraryLocation.Access">Malangnya, anda tidak mempunyai akses ke lokasi baharu</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.PrivateLibrary.LibraryLocation.Empty">Harap pilih folder kosong</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.PrivateLibrary.LibraryLocation.Move">Harap tunggu sebentar saat lokasi dipindahkan</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.PrivateLibrary.LibraryMove">Pindahkan perpustakaan Anda ke lokasi baru</system:String>
</ResourceDictionary>