<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.GeneralSettingPage.Title">GeneralSettingPage</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.ApplicationLanguage">Applikationssprog</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationLanguage.Description">Vælg det sprog, du vil se applikationen på.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup">Opstart af applikation</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Description">For at kunne afspille tapeter skal Sucrose køre i baggrunden ved opstart.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.None">Ingen</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.Normal">Normal</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.Priority">Prioritet</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.Scheduler">Planlagt</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Warning.Close">Jeg Forstår</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Warning.Title">Adgangsrettigheder Advarsel</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Warning.Message">Du skal have administrative rettigheder for at ændre opstartstilstanden.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon">Meddelelsesområdesymbol</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.Description">Synligheden af ikonet i systembakken. Sucrose vil fortsætte med at køre skjult.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.Notify.Show">Synlig</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.Notify.Hide">Skjult</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.NotifyExit">Luk alle Sucrose-programmer, når meddelelsesområdet lukkes</system:String>

