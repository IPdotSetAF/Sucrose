    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.Description">Bruges til at tilpasse baggrunden i applikationsvinduet.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackgroundImage.Title">Vælg baggrundsbillede</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackgroundImage.Select">Vælg et baggrundsbillede</system:String>
