    <system:String x:Key="Portal.GeneralSettingPage.EngineVolume.Description">Lydjustering gældende for alle tapeter.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.EngineVolume.VolumeDesktop">Afspil kun lyd, når skrivebordet er i fokus</system:String>
