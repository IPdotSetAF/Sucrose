<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.GeneralSettingPage.Title">GeneralSettingPage</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.ApplicationLanguage">Idioma de la Aplicación</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationLanguage.Description">Seleccione el idioma en el que desea ver la aplicación.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup">Inicio de la Aplicación</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Description">Sucrose necesita ejecutarse en segundo plano al inicio para reproducir el fondo de pantalla.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.None">Ninguno</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.Normal">Normal</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.Priority">Prioridad</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.ApplicationStartup.Startup.Scheduler">Programador</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon">Icono en la Bandeja de Notificaciones</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.Description">Visibilidad en la bandeja del sistema. Sucrose seguirá funcionando discretamente.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.Notify.Show">Mostrar</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.Notify.Hide">Ocultar</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.NotifyIcon.NotifyExit">Cerrar todas las aplicaciones de Sucrose cuando se cierre la bandeja del sistema</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop">Fondo de Ventana</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.Description">Se utiliza para personalizar el fondo de la ventana de la aplicación.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackgroundImage.Title">Seleccionar Imagen de Fondo</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackgroundImage.Select">Seleccionar una imagen de fondo</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackgroundImage.Filter">Archivos de Imagen (*.png;*.jpg;*.jpeg;*.tiff;*.webp;*.gif)|*.png;*.jpg;*.jpeg;*.tiff;*.webp;*.gif</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackdropStretch">Estiramiento de Fondo:</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.WindowBackdrop.BackdropOpacity">Opacidad de Fondo (%):</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.EngineVolume">Volumen del Motor</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.EngineVolume.Description">Volumen que se aplica a todos los fondos de pantalla.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.EngineVolume.VolumeDesktop">Reproducir sonido solo cuando esté enfocado en el escritorio</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.PrivateLibrary">Ubicación de la Biblioteca</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.PrivateLibrary.Description">Ubicación donde se almacenan los temas que agregas a tu biblioteca.</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.PrivateLibrary.LibraryLocation.Empty">Por favor, selecciona una carpeta vacía</system:String>
    <system:String x:Key="Portal.GeneralSettingPage.PrivateLibrary.LibraryLocation.Move">Por favor, espera mientras se cambia la ubicación</system:String>

    <system:String x:Key="Portal.GeneralSettingPage.PrivateLibrary.LibraryMove">Mueve tu biblioteca actual a la nueva ubicación</system:String>
</ResourceDictionary>