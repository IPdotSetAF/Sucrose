<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.PersonalSettingPage.Title">PersonalSettingPage</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Server">Butiksserver</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Server.Description">Anger vilken server som kommer att användas för att visa de teman som listas i butiken.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Server.ServerHint">Observera: Om {0} väljs som server måste en {2} anges för att undvika begänsningar för förfrågningar. Annars kan du uppleva problem med att komma åt teman i butiken. Om {1} väljs får du en smidigare butiksupplevelse.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Duration">Butik Uppdatering</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Duration.Description">Hur länge de listade temana i butiken ska uppdateras.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.StoreStart">Automatisk Start</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.StoreStart.Description">Om teman som laddas ner från butiken automatiskt ska ställas in som bakgrundsbild.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adult">Ocensurerat Innehåll</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Adult.Description">Om innehåll som inte är säkert för arbete (NSFW) i butiken ska visas eller döljas.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Confirm">Bekräftelse vid Radering</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Confirm.Description">Om bekräftelse ska begäras när du tar bort teman från din bibliotek.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Delete">Radera Dåliga Teman</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Delete.Description">Om dåliga eller bristfälliga teman i ditt bibliotek ska raderas automatiskt.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Delete.DeleteHint">OBS: Var försiktig med detta. Det kan leda till datamissförluster om du inte är försiktig.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.LibraryStart">Automatisk Start</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.LibraryStart.Description">Om teman som du skapar automatiskt ska ställas in som bakgrundsbild.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview">Temaförhandsgranskning</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.Description">Om teman ska förhandsgranskas när du hovrar över dem.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview.StorePreview">Butiksförhandsvisning:</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.StorePreviewHide">Dölj detaljer:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview.LibraryPreview">Biblioteksförhandsvisning:</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.LibraryPreviewHide">Dölj detaljer:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive">Anpassningsbar Layout</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.Description">Anpassa layouten eller avståndet för de visade temana.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.AdaptiveMargin">Avstånd (pixlar):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.AdaptiveLayout">Layout Ordning (antal):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Sort">Sortering av Bibliotek</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Sort.Description">Ange hur teman i din bibliotek ska sorteras och i vilket format.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Sort.SortMode">Sorteringsläge:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Sort.SortKind">Sorterings Typ:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Store">Butikssidor</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Store.Description">Bestäm antalet teman per sida i butiken.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Library">Bibliotekssidor</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Library.Description">Bestäm antalet teman per sida i ditt bibliotek.</system:String>
</ResourceDictionary>