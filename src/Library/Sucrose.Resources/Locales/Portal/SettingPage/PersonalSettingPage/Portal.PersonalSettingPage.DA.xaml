<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.PersonalSettingPage.Title">PersonalSettingPage</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Server">Butiksserver</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Server.Description">Angiver hvilken server der vil blive brugt til at vise de temaer, der er angivet i butikken.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Server.ServerHint">Bemærk: Hvis {0} vælges som server, er det nødvendigt at indtaste en {2} for at undgå anmodningsbegrænsninger. Ellers kan du opleve problemer med at få adgang til temaer i butikken. Hvis {1} vælges, får du en mere problemfri butiksoplevelse.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Duration">Butik Genindlæsning</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Duration.Description">Hvor længe temalister i butikken skal genindlæses.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.StoreStart">Automatisk Start</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.StoreStart.Description">Hvorvidt temaer, der er downloadet fra butikken, automatisk skal indstilles som tapet.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adult">Uegnet Indhold</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Adult.Description">Hvorvidt indhold, der er markeret som ikke-sikkert for arbejde (NSFW), skal vises eller ej i butikken.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Confirm">Sletningsbekræftelse</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Confirm.Description">Hvorvidt der skal anmodes om bekræftelse ved sletning af temaer i din bibliotek.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Delete">Defekte Temaer</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Delete.Description">Hvorvidt defekte eller manglende temaer i din bibliotek automatisk skal slettes.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Delete.DeleteHint">Bemærk: Vær forsigtig, da det kan medføre datatab.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.LibraryStart">Automatisk Start</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.LibraryStart.Description">Hvorvidt oprettede temaer automatisk skal indstilles som tapet.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview">Temaforhåndsvisning</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.Description">Hvorvidt temaer skal forhåndsvises ved at sveje musen over dem.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview.StorePreview">Butikforhåndsvisning:</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.StorePreviewHide">Skjul detaljer:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview.LibraryPreview">Biblioteksforhåndsvisning:</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.LibraryPreviewHide">Skjul detaljer:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Theme">Tema Tilpasning</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Theme.Description">Tilpas navnet eller beskrivelsen af de viste temaer.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Theme.TitleLength">Titellængde (tegn):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Theme.DescriptionLength">Beskrivelseslængde (tegn):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive">Tilpasning</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.Description">Tilpas layoutet eller afstanden af de viste temaer.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.AdaptiveMargin">Margeafstand (pixels):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.AdaptiveLayout">Layoutordning (antal):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Sort">Bibliotekssortering</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Sort.Description">Bestem, hvordan emnerne i din bibliotek skal sorteres og i hvilket format.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Sort.SortMode">Sorteringsmetode:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Sort.SortKind">Sorteringsart:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Store">Butikssideinddeling</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Store.Description">Bestem antallet af temaer på hver side i butikken.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Library">Biblioteksinddeling</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Library.Description">Bestem antallet af temaer på hver side i din bibliotek.</system:String>
</ResourceDictionary>