<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.WallpaperCycling.Title">वॉलपेपर साइकिलिंग</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Close">बंद करें</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Cycling">स्वचालित वॉलपेपर साइकिलिंग</system:String>
