<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.WallpaperCycling.Title">Wallpaper Cycling</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Close">Close</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Cycling">Automatic Wallpaper Cycling</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Cycling.Description">Whether the wallpaper changes automatically or not.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Time">Time Interval</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Time.Description">The time interval after which the wallpaper changes.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Transition">Transition Mode</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Transition.Description">Sets the transition mode between wallpapers.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.List">Cycle List</system:String>
    <system:String x:Key="Portal.WallpaperCycling.List.Description">Resets the cycle list to default.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.List.Reset">Reset</system:String>
</ResourceDictionary>