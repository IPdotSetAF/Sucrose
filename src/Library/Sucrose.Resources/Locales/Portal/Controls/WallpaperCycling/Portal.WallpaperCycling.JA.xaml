<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.WallpaperCycling.Title">壁紙サイクリング</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Close">閉じる</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Cycling">自動壁紙サイクリング</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Cycling.Description">壁紙が自動的に変わるかどうか。</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Time">時間間隔</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Time.Description">壁紙が変わる時間間隔。</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Transition">トランジションモード</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Transition.Description">壁紙間のトランジションモードを設定します。</system:String>

    <system:String x:Key="Portal.WallpaperCycling.List">サイクルリスト</system:String>
    <system:String x:Key="Portal.WallpaperCycling.List.Description">サイクルリストをデフォルトにリセットします。</system:String>

    <system:String x:Key="Portal.WallpaperCycling.List.Reset">リセット</system:String>
</ResourceDictionary>