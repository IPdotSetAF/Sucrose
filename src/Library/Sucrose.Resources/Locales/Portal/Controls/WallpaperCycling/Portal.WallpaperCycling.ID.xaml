<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.WallpaperCycling.Title">Pemutaran Wallpaper</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Close">Tutup</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Cycling">Pemutaran Wallpaper Otomatis</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Cycling.Description">Apakah wallpaper berubah secara otomatis atau tidak.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Time">Interval Waktu</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Time.Description">Interval waktu setelah wallpaper berubah.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Transition">Mode Transisi</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Transition.Description">Mengatur mode transisi antara wallpaper.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.List">Daftar Siklus</system:String>
    <system:String x:Key="Portal.WallpaperCycling.List.Description">Mereset daftar siklus ke default.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.List.Reset">Reset</system:String>
