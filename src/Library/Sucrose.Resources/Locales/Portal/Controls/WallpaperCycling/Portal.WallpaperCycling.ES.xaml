<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.WallpaperCycling.Title">Cambio de Fondo de Pantalla</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Close">Cerrar</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Cycling">Cambio Automático de Fondo de Pantalla</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Cycling.Description">Si el fondo de pantalla cambia automáticamente o no.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Time">Intervalo de Tiempo</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Time.Description">El intervalo de tiempo después del cual cambia el fondo de pantalla.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.Transition">Modo de Transición</system:String>
    <system:String x:Key="Portal.WallpaperCycling.Transition.Description">Establece el modo de transición entre fondos de pantalla.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.List">Lista de Ciclos</system:String>
    <system:String x:Key="Portal.WallpaperCycling.List.Description">Restablece la lista de ciclos a los valores predeterminados.</system:String>

    <system:String x:Key="Portal.WallpaperCycling.List.Reset">Restablecer</system:String>
</ResourceDictionary>