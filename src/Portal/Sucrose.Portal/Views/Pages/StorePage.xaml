<Page
    x:Class="Sucrose.Portal.Views.Pages.StorePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Sucrose.Portal.Views.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    Title="{DynamicResource Portal.StorePage.Title}"
    d:DataContext="{d:DesignInstance local:StorePage,
                                     IsDesignTimeCreatable=False}"
    mc:Ignorable="d">

    <Grid x:Name="GridStore" Loaded="GridStore_Loaded">
        <Frame x:Name="FrameStore" Visibility="Collapsed" />
        <ui:ProgressRing
            x:Name="ProgressStore"
            Width="100"
            Height="100"
            IsIndeterminate="True" />
    </Grid>
</Page>